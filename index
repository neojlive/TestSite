        var chrono = $('.chronoline'),
                chronoWidth = $('.chronoline').width(),
                tline = chrono.find('.tline'),
                tlineMover = tline.find('.mover'),
                tlineDiv = tline.find('.division'),
                textBox = chrono.find('.box')

        var max = 0;
        function moveLine() {

            setTimeout(function() {

                tlineMover.animate({
                    'padding-left': 0
                }, 700, 'linear', function() {
                    tline
                            .find('.division').last()
                            .insertAfter(tline.find('.division').last());
                    tlineMover.css('padding-left', 73);
                    moveLine();
                });
                $('.top-bar .box, .bottom-bar .box').each(function() {
                    var $this = $(this);
                    if ((parseFloat($this.css('left')) - 73 < 0)) {

                        $this.fadeOut();
                        var round = Math.round((Math.random() * (100 - 10) + 10));
                        $this.parent().find('.box').each(function() {

                            if (parseFloat($(this).css('left')) > max) {
                                max = parseFloat($(this).css('left'));
                            }
                        });
                        $this.css('left', max + 222 + round);
                        max = 0;
                    }

                    $this.animate({
                                'left':  parseFloat($this.css('left')) - 73},
                            700, 'linear', function() {

                                if (parseFloat($this.css('left')) + 222 < chronoWidth) {
                                    $this.delay(200).fadeIn();
                                }
                            });
                });
            }, 10000);
        }

        function getPosition() {
            var min = 10;
            var max = 100;
            var width = 0;
            var boxTop = $('.top-bar .box');
            var boxBotton = $('.bottom-bar .box');
            var n = 222;
            var is1 = 0
            var is2 = 0


            for (var i = 0; i < 5; i++) {

                var round1 = Math.round((Math.random() * (max - min) + min));
                var round2 = Math.round((Math.random() * (max - min) + min));
                if (i == 0) {n = 0; } else {n = 222};
                is1 = is1 + n + round1;
                is2 = is2 + n + round2;
                boxTop.eq(i).css('left', is1);
                if (parseFloat(boxTop.eq(i).css('left')) + 222 + round1 > chronoWidth) {
                    boxTop.eq(i).hide()
                }
                boxBotton.eq(i).css('left', is2);
                if (parseFloat(boxBotton.eq(i).css('left')) + 222 + round2 > chronoWidth) {
                    boxBotton.eq(i).hide()
                }


            }


        }

        getPosition();
        moveLine();

    });

    var lastPayments='1460097327,1460097461,1460097462,1460097472,1460097579,1460097595,1460097756,1460097761,1460097795,1460097797,1460097799,1460097855,1460097882,1460097896'; 

    var indexPayments=0;
    var counterVal=0;
    var lastPamentsArr=lastPayments.split(',');
    var refresh=lastPamentsArr.length;
    var operationCount = $('.all-pays').children('p').html();
    operationCount= parseInt(operationCount.replace(/\D+/g,""));

    function updateCounter (){


        if(indexPayments==refresh-1)
        {
            indexPayments=0;
        }


        if(((lastPamentsArr[indexPayments]*1)+counterVal)>=lastPamentsArr[indexPayments+1])
        {
            counterVal=0;
            indexPayments++;
            operationCount++;
            $('.pagEnumerator-number-cont-v').html(operationCount.toString().replace(/(\d{1,3})(?=(?:\d{3})+$)/g, '$1 '));
        }
        $('.pagEnumerator-time-cont').html(counterVal);

        counterVal++;


        setTimeout(updateCounter,1000);

    }
</script>

