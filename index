    $('.callBackContent #send').click(function () {
        var valid = true;
        $('form.regContent').find('input').each(function () {
            if (!$(this).val().match($(this).prop('pattern'))) {
                valid = false;
                $(this).addClass('error');
            }
        });
        if (valid) {
            var data = {
                action: 'callMe',
                phonenum: $('[name="phonenum"]').val(),
                calltime: $('.knopikWrap .selectBox-label').text(),
                url: 'ru'
            };
            sendAjax(data).success(function (answer) {
                if (answer.success == 'false') {
                    for (var i in answer.data) {
                        $('input[name=' + i + ']').addClass('error');
                    }
                } else {
                    $('.sidebar.harmonica').hide();
                    $('.harmonica').after(answer.data);
                    $('#blueSuccessContent').show();
                }
            });
        }
    });
    /**
     * Написать сообщение
     */
    $('.aui-button-content .aui-button-input').click(function () {
        var valid = true;
        $('form.messagemail').find('input[type=tel], input[type=email], textarea').each(function () {
            if (($(this).val() == '' && $(this).attr('required') == 'required') || ($(this).val() != '' && !$(this).val().match($(this).prop('pattern')))) {
                valid = false;
                $(this).addClass('error');
            } else {
                $(this).removeClass('error');
            }
        });
        var phonenum = '';
        var email = '';
        if (valid) {
            var data = {
                action: 'messageMail',
                message: $('[name="message"]').val(),
                phonenum2: phonenum || $('[name="phonenum2"]').val(),
                email: email || $('[name="email"]').val(),
                img: $('.path').text(),
                url: 'ru'
            };
            sendAjax(data).success(function (answer) {
                if (answer.success == 'false') {
                    for (var i in answer.data) {
                        $('input[name=' + i + ']').addClass('error');
                    }
                } else {
                    $('.sidebar.harmonica').hide();
                    $('.harmonica').after(answer.data);
                    $('#blueSuccessContent').show();
                }
            });
        }


    })

