    $('.knopikWrap .file_upload input[type="file"]').change(function () {
        uploadFiles('application/views/blocks/widgets/upload.php', this.files);
        //        uploadFiles('/ajax', this.files);
    });
    function uploadFiles(url, files) {
        var formData = new FormData();
        for (var i = 0, file; file = files[i]; ++i) {
            formData.append('fileUpload', file);
        }

        var xhr = new XMLHttpRequest();
        xhr.open('POST', url, true);
        xhr.onload = function (e) {
            var imgName = JSON.parse(this.response).name;
            var path = JSON.parse(this.response).path;
            var status = JSON.parse(this.response).status;
            var status_error = JSON.parse(this.response).text;
            $('#textFileUpload .imgName, #textFileUpload .path, #textFileUpload .error').remove();
            if (status == 'success') {
                $('#textFileUpload').append('<span class="imgName">' + imgName + '</span>');
                $('#textFileUpload').append('<span class="path" style="display:none">' + path + '</span>');
            } else {
                $('#textFileUpload').append('<span class="error">' + status_error + '</span>');
            }
        };
        xhr.send(formData); // multipart/form-data
    }


