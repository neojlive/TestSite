    $('.knopikBut').click(function () {
        if ($(this).closest('.knopikWrap').hasClass('hidden')) {
            $('.knopikWrap')
                    .animate({right: 0})
                    .removeClass('hidden');
        } else {
            $('.knopikWrap')
                    .animate({right: - 260})
                    .addClass('hidden');
        }
    });
    $('.harFull').click(function () {
        $('.knopikWrap form input, .knopikWrap form textarea').each(function () {
            $(this).removeClass('error').val('');
        });
        if ($(this).hasClass('harOpen')) {
            $(this).removeClass('harOpen').next().slideUp('slow');
        } else {
            $('.harOpen').removeClass('harOpen').next().slideUp('slow');
            $(this).addClass('harOpen').next().slideDown('slow');
        }

        return false;
    });
    /**
