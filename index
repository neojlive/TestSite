<script type="text/javascript">
    jQuery(document).ready(function() {
        jQuery('#scrollup .up').mouseover(function() {
            jQuery(this).animate({opacity: 0.65}, 100);
        }).mouseout(function() {
            jQuery(this).animate({opacity: 1}, 100);
        }).click(function() {
            window.scroll(0, 0);
            return false;
        });
        jQuery(window).scroll(function() {

            var delta = $('body').height() - jQuery(document).scrollTop();
            if (delta > 700) {
                if (jQuery(document).scrollTop() > 400) {
                    $("#scrollup").css({ bottom: "10px" });
                    jQuery('#scrollup').fadeIn('fast');
                } else {
                    jQuery('#scrollup').fadeOut('fast');
                }
            } else {
                if ($("#footer .base").hasClass("expanded")){
                    $("#scrollup").css({ bottom: "295px" });
                } else {
                    $("#scrollup").css({ bottom: "90px" });
                }
            }

        });
    });        </script>

<script>
    function updateAdaptiveTimeCounter(){

        $.ajax({
            type: "GET",
            url: "/adaptivePageCounter.php",
            cache: false,
            dataType: "text",
            success: function(data) {
                data=JSON.parse(data)

                $('.pagEnumerator-time-cont').html(' '+data.num+' ');
                $('.pagEnumerator-time-text').html(data.val.ru);


            }
        });

        setTimeout(updateAdaptiveTimeCounter,3000);

    }

