<script type="text/javascript">
    $(".phone-masks").mask("?+380 (99) 999 99 99");
    $(".phone-mask-question").mask("?+380 (99) 999 99 99");
    $('input.phone-masks').bind('paste', function () { $('input.phone-masks').val(''); });
    $('input.phone-mask-question').bind('paste', function () { $('input.phone-mask-question').val(''); });
    function validСellPhone() {
        var phone = $("input.phonenum2").val();
        var _ptrn = new RegExp('_');
        if (phone.length != 0){
            if (!_ptrn.test(phone)){
                if ((phone.length == 19) && (phone != '+380 (00) 000 00 00')){
                    $("input.phonenum2").removeClass('error').css("border", "#cccccc solid 1px");
                    $("input.phonenum2").focus();
                } else {
                    $("input.phonenum2").addClass('error').css("border", "#dd3e2b solid 1px").val(phone);
                }
            }
        } else {
            $("input.phonenum2").addClass('error').css("border", "#dd3e2b solid 1px");
        }
    }

    $('select[name="theme"]').selectBox();
    /**
     * Анимация меню
